{
  "name": "100cims",
  "main": "expo-router/entry",
  "version": "2.2.0",
  "private": false,
  "license": "MIT",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "start": "npx expo start --clear",
    "start:prod": "npx expo start --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "translations:extract": "formatjs extract './{components,app}/**/*.ts*' --ignore='**/*.d.ts' --out-file translations/raw-en.json",
    "translations:generate": "formatjs compile './translations/raw-en.json' --out-file translations/en.json",
    "translations": "yarn translations:extract && yarn translations:generate",
    "generate-api-types": "openapi-typescript http://localhost:3000/api/swagger/json -o ./types/api.ts",
    "db:push": "EXPO_PUBLIC_API_URL=http://localhost:8081/ DATABASE_URL=postgresql://admin:admin@localhost:5433/my_local_db npx drizzle-kit push",
    "db:push:prod": "npx drizzle-kit push",
    "db:local:up": "docker-compose up -d",
    "build:development:ios": "npx expo run:ios",
    "build:development:android": "npx expo run:android",
    "build:api": "npx expo export -p web"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@jvidalv/react-analytics": "^1.0.1",
