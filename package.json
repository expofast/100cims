{
  "name": "100cims",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npx expo start --clear",
    "start:prod": "npx expo start --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "translations:extract": "formatjs extract './{components,app}/**/*.ts*' --ignore='**/*.d.ts' --out-file translations/raw-en.json",
    "translations:generate": "formatjs compile './translations/raw-en.json' --out-file translations/en.json",
    "translations": "yarn translations:extract && yarn translations:generate",
    "db:push": "npx drizzle-kit push",
    "db:local:up": "docker-compose up -d",
    "build:development:ios": "npx expo run:ios",
    "build:development:android": "npx expo run:android",
    "update:production:all": "npx expo run:android"
  },
  "jest": {
    "preset": "jest-expo"
  },
